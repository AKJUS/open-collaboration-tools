{
  "name": "open-collaboration-rpc",
  "version": "0.0.3",
  "description": "Open Collaboration RPC Implementation",
  "dependencies": {
    "socket.io-client": "^4.5.3"
  },
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/common/index.js",
  "types": "./lib/common/index.d.ts",
  "exports": {
    ".": {
        "types": "./lib/common/index.d.ts",
        "require": "./lib/common/index.js"
    },
    "./browser/websocket": {
        "types": "./lib/browser/websocket-transport.d.ts",
        "require": "./lib/browser/websocket-transport.js"
    },
    "./browser/socket.io": {
        "types": "./lib/browser/socket-io-transport.d.ts",
        "require": "./lib/browser/socket-io-transport.js"
    }
  },
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeFox/open-collaboration-server.git"
  },
  "bugs": {
    "url": "https://github.com/TypeFox/open-collaboration-server/issues"
  },
  "homepage": "https://github.com/TypeFox/open-collaboration-server"
}
